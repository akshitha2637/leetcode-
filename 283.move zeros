void moveZeroes(int* nums, int numsSize) {
    int i,j,temp;
while (j<numsSize && nums[j]!=0){
    j++;
}
i=j+1;
while (i<numsSize){
    if (nums[i]!=0){
        temp = nums[j];
        nums[j]=nums[i];
        nums[i]=temp;
        i++;
        j++;
    }
    else{
        i++;
    }
    
}
    
}
_____________________________
class Solution(object):
    def moveZeroes(self, nums):
        j=0
        while j<len(nums) and nums[j]!=0:
            j+=1
        i=j+1
        while i<len(nums):
            if (nums[i]!=0):
                nums[j],nums[i]=nums[i],nums[j]
                i+=1
                j+=1
            else:
                 i+=1
        
